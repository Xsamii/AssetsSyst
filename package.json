{
  "name": "maintenance-pms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_orbits": "ng serve --configuration=orbits -o",
    "start_hail": "ng serve --configuration=hail -o",
    "build": "ng build",
    "build_dev": "ng build --configuration development  --output-path=dist/development",
    "build_test": "ng build --configuration test  --output-path=test",
    "test": "ng test",
    "build_hail": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration hail --base-href /namzaga/ --output-path=hail",
    "build_orbits": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration orbits --output-path=orbits",
    "build_test_hail": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration test-hail --output-path=test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/google-maps": "^18.0.2",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@asymmetrik/ngx-leaflet": "^17.0.0",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@microsoft/signalr": "^8.0.7",
    "@ng-select/ng-select": "^10.0.3",
    "@types/file-saver": "^2.0.7",
    "file-saver": "^2.0.5",
    "highcharts": "^11.4.6",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet.markercluster": "^1.5.3",
    "marked": "^16.3.0",
    "moment": "^2.30.1",
    "ng-otp-input": "^1.9.3",
    "ngx-filesaver": "^17.0.0",
    "ngx-spinner": "^17.0.0",
    "primeicons": "^6.0.1",
    "primeng": "^16.9.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet-draw": "^1.0.12",
    "autoprefixer": "^10.4.19",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.0.2"
  }
}
