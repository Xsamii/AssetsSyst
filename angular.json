{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "maintenance-pms": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/maintenance-pms",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "node_modules/leaflet-draw/dist/leaflet.draw.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/ngx-spinner/animations/square-jelly-box.css",
              "node_modules/@arcgis/core/assets/esri/themes/light/main.css",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/leaflet/dist/leaflet.js",
              "node_modules/leaflet.markercluster/dist/leaflet.markercluster.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            },
            "test": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ]
            },
            "test-hail": {
  "styles": [
    "node_modules/leaflet-draw/dist/leaflet.draw.css",
    "node_modules/leaflet/dist/leaflet.css",
    "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
    "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
    "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
    "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
    "node_modules/primeng/resources/primeng.min.css",
    "node_modules/primeicons/primeicons.css",
    "node_modules/ngx-spinner/animations/square-jelly-box.css",
    "node_modules/@arcgis/core/assets/esri/themes/light/main.css",
    "src/styles.hail.scss"
  ],
  "assets": [
    "src/favicon.ico",
    "src/assets",
    {
      "input": "src/clients/hail/assets/images",
      "output": "assets/images",
      "glob": "*.*"
    },
    {
      "input": "src/clients/hail/assets/icons",
      "output": "assets/icons",
      "glob": "*.*"
    }
  ]
},
            "hail": {
              "styles": [
                "node_modules/leaflet-draw/dist/leaflet.draw.css",
                "node_modules/leaflet/dist/leaflet.css",
                "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
                "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
                "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
                "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
                "node_modules/primeng/resources/primeng.min.css",
                "node_modules/primeicons/primeicons.css",
                "node_modules/ngx-spinner/animations/square-jelly-box.css",
                "node_modules/@arcgis/core/assets/esri/themes/light/main.css",
                "src/styles.hail.scss"
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hail.ts"

                }
              ],
                  "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "input": "src/clients/hail/assets/images",
                  "output": "assets/images",
                  "glob": "*.*"
                },
                {
                  "input": "src/clients/hail/assets/icons",
                  "output": "assets/icons",
                  "glob": "*.*"
                }
              ]
            },
            "orbits": {
              "styles": [
                "node_modules/leaflet-draw/dist/leaflet.draw.css",
                "node_modules/leaflet/dist/leaflet.css",
                "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
                "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
                "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
                "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
                "node_modules/primeng/resources/primeng.min.css",
                "node_modules/primeicons/primeicons.css",
                "node_modules/ngx-spinner/animations/square-jelly-box.css",
                "node_modules/@arcgis/core/assets/esri/themes/light/main.css",
                "src/styles.orbits.scss"
              ],

                  "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "input": "src/clients/orbits/assets/images",
                  "output": "assets/images",
                  "glob": "*.*"
                },
                {
                  "input": "src/clients/orbits/assets/icons",
                  "output": "assets/icons",
                  "glob": "*.*"
                }
              ],
               "fileReplacements": [
                {
                  "replace": "src/environments/environment.development.ts",
                  "with": "src/environments/environment.ts"

                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "maintenance-pms:build:production"
            },
            "development": {
              "browserTarget": "maintenance-pms:build:development"
            },
            "test": {
              "browserTarget": "maintenance-pms:build:test"
            },
            "hail": {
              "browserTarget": "maintenance-pms:build:hail"
            },
            "orbits": {
              "browserTarget": "maintenance-pms:build:orbits"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "maintenance-pms:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
